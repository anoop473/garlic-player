<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<smil>
	<head>
		<meta name="title" content="Simple SMIL for testing excl with peers stop" />
		<layout>
			<root-layout id="display:0" width="1920" height="1080" />
			<region regionName="screen" top="0" left="0" width="1920" height="1080" z-index="1" backgroundColor="#000" />
		</layout>
	</head>
	<body>
	


		<img xml:id="img begin" src="../images/begin.jpg" fit="meetbest" dur="2s" />
		<excl repeatCount="indefinite" xml:id="excl1">
			<priorityClass peers="stop">
				<img xml:id="imgA0001" src="../images/0001.jpg" dur="1s" fit="meetbest" />
				<img xml:id="imgA0002" src="../images/0002.jpg" dur="1s" fit="meetbest" />
				<img xml:id="imgA0003" src="../images/0003.jpg" dur="1s" fit="meetbest" />
				<img xml:id="imgA0004" src="../images/0004.jpg" dur="1s" fit="meetbest" />
				<img xml:id="imgA0005" src="../images/0005.jpg" dur="1s" fit="meetbest" />
			</priorityClass>	
		</excl>	
		<img  xml:id="img_end" src="../images/end.jpg" fit="meetbest" dur="2s" />
	</body>
</smil>
